    "+", 
    RowBox[{"2", 
     RowBox[{"(", 
      RowBox[{
       SqrtBox[
        FractionBox[
         RowBox[{"1", "-", 
          RowBox[{"\[Alpha]", " ", "g", " "}]}], 
         RowBox[{"1", "-", "\[Alpha]"}]]], "+", "1"}], ")"}]}]}]]}]], "Input",\

