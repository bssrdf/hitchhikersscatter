Cell[BoxData[
 RowBox[{
  RowBox[{"\[Phi]", "[", 
   RowBox[{"x_", ",", "\[Alpha]_", ",", "\[CapitalSigma]t_"}], "]"}], ":=", 
  RowBox[{
   RowBox[{"LR", "[", 
    RowBox[{"x", ",", "\[Alpha]", ",", "\[CapitalSigma]t"}], "]"}], "+", 
