Cell[BoxData[
 RowBox[{
  RowBox[{"R", "[", 
   RowBox[{"a_", ",", "\[Alpha]_", ",", "\[CapitalSigma]t_"}], "]"}], ":=", 
  FractionBox["\[Alpha]", 
   RowBox[{
    RowBox[{"2", 
     SqrtBox[
      RowBox[{"1", "-", "\[Alpha]"}]], 
     RowBox[{"Coth", "[", 
      RowBox[{"\[CapitalSigma]t", " ", "a", " ", 
       SqrtBox[
        RowBox[{"1", "-", "\[Alpha]"}]]}], "]"}]}], "-", "\[Alpha]", "+", 
    "2"}]]}]], "Input",
 CellChangeTimes->{{3.6211788319809732`*^9, 3.621178886778017*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"T", "[", 
   RowBox[{"a_", ",", "\[Alpha]_", ",", "\[CapitalSigma]t_"}], "]"}], ":=", 
  FractionBox[
   RowBox[{"2", 
    SqrtBox[
     RowBox[{"1", "-", "\[Alpha]"}]]}], 
   RowBox[{
    RowBox[{"2", 
     SqrtBox[
      RowBox[{"1", "-", "\[Alpha]"}]], 
     RowBox[{"Cosh", "[", 
      RowBox[{"\[CapitalSigma]t", " ", "a", 
       SqrtBox[
        RowBox[{"1", "-", "\[Alpha]"}]]}], "]"}]}], "-", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"\[Alpha]", "-", "2"}], ")"}], 
     RowBox[{"Sinh", "[", 
      RowBox[{"\[CapitalSigma]t", " ", "a", 
       SqrtBox[
        RowBox[{"1", "-", "\[Alpha]"}]]}], "]"}]}]}]]}]], "Input",
 CellChangeTimes->{{3.62137522443165*^9, 3.621375306559001*^9}}],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"R", "[", 
    RowBox[{"a_", ",", "\[Alpha]_", ",", "\[CapitalSigma]t_", ",", "n_"}], 
    "]"}], ":=", 
   RowBox[{
    SuperscriptBox["\[Alpha]", "n"], 
    RowBox[{"(", 
     RowBox[{
      RowBox[{"SeriesCoefficient", "[", 
       RowBox[{
        RowBox[{"R", "[", 
         RowBox[{"a", ",", "A", ",", "\[CapitalSigma]t"}], "]"}], ",", 
