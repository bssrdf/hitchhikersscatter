     RowBox[{
      RowBox[{"-", 
       RowBox[{"Abs", "[", "x", "]"}]}], " ", 
      SqrtBox[
       RowBox[{"1", "-", "\[Alpha]"}]], " ", "\[CapitalSigma]t"}]], " ", 
    RowBox[{"(", 
     RowBox[{"1", "-", 
      RowBox[{
