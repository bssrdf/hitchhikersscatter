Cell["Grosjean 1958b approximation", "Subsection",
 CellChangeTimes->{{3.589404972543141*^9, 3.5894049772359753`*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"HGrosjean", "[", 
   RowBox[{"w_", ",", "u_"}], "]"}], ":=", 
  RowBox[{
   RowBox[{"1", "+", 
    RowBox[{
     FractionBox[
      RowBox[{"w", " ", "u"}], "2"], 
     RowBox[{"Log", "[", 
      FractionBox[
       RowBox[{"1", "+", "u"}], "u"], "]"}]}], "+", 
    FractionBox[
     RowBox[{"2", " ", 
      SuperscriptBox["w", "2"], "u"}], 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"1", "+", 
        RowBox[{"K", " ", "u"}]}], ")"}], 
      RowBox[{"(", 
       RowBox[{
        FractionBox[
         RowBox[{"13", "-", 
          RowBox[{"5", "w"}]}], "8"], "+", 
        RowBox[{
         FractionBox["2", "3"], 
         RowBox[{"(", 
          RowBox[{"2", "-", "w"}], ")"}], "K"}]}], ")"}]}]], "-", 
    RowBox[{
     FractionBox[
      RowBox[{
       SuperscriptBox["w", "2"], "u"}], 
      RowBox[{"(", 
       RowBox[{
        FractionBox[
         RowBox[{"13", "-", 
          RowBox[{"5", "w"}]}], "8"], "+", 
        RowBox[{
         FractionBox["2", "3"], 
         RowBox[{"(", 
          RowBox[{"2", "-", "w"}], ")"}], "K"}]}], ")"}]], 
     RowBox[{"(", 
      RowBox[{
       FractionBox["17", "32"], "+", 
       RowBox[{
        FractionBox["15", "16"], "u"}], "-", 
       RowBox[{"u", 
        RowBox[{"(", 
         RowBox[{"1", "+", 
          RowBox[{
           FractionBox["15", "16"], "u"}]}], ")"}], 
        RowBox[{"Log", "[", 
         FractionBox[
          RowBox[{"1", "+", "u"}], "u"], "]"}]}]}], ")"}]}]}], "/.", 
   RowBox[{"K", "\[Rule]", 
    SuperscriptBox[
     RowBox[{"(", 
      FractionBox[
       RowBox[{"3", 
        RowBox[{"(", 
         RowBox[{"1", "-", "w"}], ")"}]}], 
       RowBox[{"2", "-", "w"}]], ")"}], 
     RowBox[{"1", "/", "2"}]]}]}]}]], "Input",
 CellChangeTimes->{{3.589404979924155*^9, 3.589405165698153*^9}, {
  3.5894054160447693`*^9, 3.589405416659287*^9}}]
}, Open  ]],

