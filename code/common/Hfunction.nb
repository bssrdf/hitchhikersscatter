  RowBox[{
   RowBox[{
    FractionBox["1", 
     RowBox[{"j", "+", "1"}]], 
    FractionBox["2", 
     RowBox[{"1", "+", "y"}]], 
    RowBox[{"(", 
     RowBox[{"1", "+", 
      RowBox[{
       FractionBox["j", 
        RowBox[{"2", 
         RowBox[{"(", 
          RowBox[{"j", "+", "2"}], ")"}]}]], 
       FractionBox[
        RowBox[{"1", "-", "y"}], 
        RowBox[{"1", "+", "y"}]]}]}], ")"}]}], "/.", 
   RowBox[{"y", "\[Rule]", 
    SqrtBox[
     RowBox[{"1", "-", "c"}]]}]}]}]], "Input",
