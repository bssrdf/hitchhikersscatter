Cell[BoxData[
 RowBox[{
  RowBox[{"Hmoment1", "[", "c_", "]"}], ":=", 
  RowBox[{
   RowBox[{"(", 
    RowBox[{"1", "-", 
     SqrtBox[
      RowBox[{"1", "-", "c"}]]}], ")"}], 
   FractionBox["2", "c"]}]}]], "Input",
