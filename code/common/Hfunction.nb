Cell[BoxData[{
 RowBox[{
  RowBox[{"Clear", "[", "H", "]"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"H", "[", 
   RowBox[{"\[Alpha]_", ",", "u_"}], "]"}], ":=", 
  RowBox[{"Exp", "[", 
   RowBox[{
    FractionBox[
     RowBox[{"-", "u"}], "Pi"], 
    RowBox[{"NIntegrate", "[", 
     RowBox[{
      FractionBox[
       RowBox[{"Log", "[", 
        RowBox[{"1", "-", 
         RowBox[{"\[Alpha]", " ", "t", " ", 
          RowBox[{"Cot", "[", "t", "]"}]}]}], "]"}], 
       RowBox[{
        SuperscriptBox[
         RowBox[{"Cos", "[", "t", "]"}], "2"], "+", 
        RowBox[{
         SuperscriptBox["u", "2"], 
         SuperscriptBox[
          RowBox[{"Sin", "[", "t", "]"}], "2"]}]}]], ",", 
      RowBox[{"{", 
       RowBox[{"t", ",", "0", ",", 
        FractionBox["Pi", "2"]}], "}"}]}], "]"}]}], "]"}]}]}], "Input",
 CellChangeTimes->{
  3.578209757735074*^9, 3.5825002943840857`*^9, {3.678241691463173*^9, 
   3.678241699401926*^9}}],

Cell["\<\
Special cases for no absorption (\[Alpha] = 1):\[LineSeparator][Wiener and \
Hopf 1931]\
\>", "Text",
 CellChangeTimes->{{3.680216188342749*^9, 3.680216236261297*^9}, {
  3.6802186417181377`*^9, 3.6802186439178677`*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"HWienerHopf", "[", "u_", "]"}], ":=", 
  RowBox[{
   RowBox[{"(", 
    RowBox[{"1", "+", "u"}], ")"}], 
   RowBox[{"Exp", "[", 
    RowBox[{
     FractionBox["u", "Pi"], 
     RowBox[{"NIntegrate", "[", 
      RowBox[{
       FractionBox[
        RowBox[{"Log", "[", 
         RowBox[{
          SuperscriptBox[
           RowBox[{"Sin", "[", "t", "]"}], "2"], "/", 
          RowBox[{"(", 
           RowBox[{"1", "-", " ", 
            RowBox[{"t", " ", 
             RowBox[{"Cot", "[", "t", "]"}]}]}], ")"}]}], "]"}], 
        RowBox[{
         SuperscriptBox[
          RowBox[{"Cos", "[", "t", "]"}], "2"], "+", 
         RowBox[{
          SuperscriptBox["u", "2"], 
          SuperscriptBox[
           RowBox[{"Sin", "[", "t", "]"}], "2"]}]}]], ",", 
       RowBox[{"{", 
        RowBox[{"t", ",", "0", ",", 
         FractionBox["Pi", "2"]}], "}"}]}], "]"}]}], "]"}]}]}]], "Input"],

Cell["[Placzek 1947]", "Text",
 CellChangeTimes->{{3.680218653334915*^9, 3.680218658229488*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"HPlaczek", "[", "u_", "]"}], ":=", 
  RowBox[{
   FractionBox["1", 
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{"1", "+", "u"}], ")"}], 
     RowBox[{"1", "/", "2"}]]], 
   RowBox[{"Exp", "[", 
    RowBox[{
     FractionBox["1", "Pi"], 
     RowBox[{"NIntegrate", "[", 
      RowBox[{
       FractionBox[
        RowBox[{"t", " ", 
         RowBox[{"ArcTan", "[", 
          RowBox[{"u", " ", 
           RowBox[{"Tan", "[", "t", "]"}]}], "]"}]}], 
        RowBox[{"1", "-", 
         RowBox[{"t", " ", 
          RowBox[{"Cot", "[", "t", "]"}]}]}]], ",", 
       RowBox[{"{", 
        RowBox[{"t", ",", "0", ",", 
         FractionBox["Pi", "2"]}], "}"}]}], "]"}]}], "]"}]}]}]], "Input"],

