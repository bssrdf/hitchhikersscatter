Cell[CellGroupData[{

Cell["Approximate H-function", "Subsection",
 CellChangeTimes->{{3.5781028301248407`*^9, 3.578102834541438*^9}}],

Cell["Hapke approximation 1", "Text",
 CellChangeTimes->{{3.589404945185061*^9, 3.589404948404126*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"H1", "[", 
   RowBox[{"w_", ",", "u_"}], "]"}], ":=", 
  FractionBox[
   RowBox[{"1", "+", 
    RowBox[{"2", "u"}]}], 
   RowBox[{"1", "+", 
    RowBox[{"2", "u", 
     SqrtBox[
      RowBox[{"1", "-", "w"}]]}]}]]}]], "Input",
 CellChangeTimes->{{3.5824912057935743`*^9, 3.582491250879169*^9}}],

Cell["Hapke approximation 2", "Text",
 CellChangeTimes->{{3.5894049528472347`*^9, 3.589404955721003*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"Clear", "[", "H2", "]"}], ";", 
  RowBox[{
   RowBox[{"H2", "[", 
    RowBox[{"w_", ",", "u_"}], "]"}], ":=", 
   RowBox[{
    RowBox[{
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"1", "-", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"1", "-", "y"}], ")"}], "u", 
         RowBox[{"(", 
          RowBox[{"n", "+", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"1", "-", 
              FractionBox["n", "2"], "-", 
              RowBox[{"n", " ", "u"}]}], ")"}], 
            RowBox[{"Log", "[", 
             FractionBox[
              RowBox[{"1", "+", "u"}], "u"], "]"}]}]}], ")"}]}]}], ")"}], 
      RowBox[{"-", "1"}]], "/.", 
     RowBox[{"n", "\[Rule]", 
      FractionBox[
       RowBox[{"1", "-", "y"}], 
       RowBox[{"1", "+", "y"}]]}]}], "/.", 
    RowBox[{"y", "\[Rule]", 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"1", "-", "w"}], ")"}], 
      RowBox[{"1", "/", "2"}]]}]}]}]}]], "Input",
 CellChangeTimes->{{3.5863359359252996`*^9, 3.586335937700029*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"V", "[", 
   RowBox[{"s_", ",", "c_"}], "]"}], ":=", 
  RowBox[{"1", "-", 
   RowBox[{
    FractionBox["c", 
     RowBox[{"2", " ", "s"}]], 
    RowBox[{"Log", "[", 
     FractionBox[
      RowBox[{"1", "+", "s"}], 
      RowBox[{"1", "-", "s"}]], "]"}]}]}]}]], "Input",
 CellChangeTimes->{{3.583038034740556*^9, 3.583038045672323*^9}, {
  3.583038077499847*^9, 3.583038083454585*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"vexact", "[", 
   RowBox[{"c_", "?", "NumericQ"}], "]"}], ":=", 
  RowBox[{
   RowBox[{
    RowBox[{"FindRoot", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"V", "[", 
        RowBox[{"s", ",", "c"}], "]"}], "\[Equal]", "0"}], ",", 
      RowBox[{"{", 
       RowBox[{"s", ",", 
        RowBox[{"10", "^", 
         RowBox[{"-", "10"}]}], ",", 
        RowBox[{"1", "-", 
         RowBox[{"10", "^", 
          RowBox[{"-", "10"}]}]}]}], "}"}], ",", 
      RowBox[{"Method", "\[Rule]", "\"\<Brent\>\""}]}], "]"}], "[", 
    RowBox[{"[", "1", "]"}], "]"}], "[", 
   RowBox[{"[", "2", "]"}], "]"}]}]], "Input"]
}, Open  ]],

