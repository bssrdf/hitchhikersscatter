Cell["\<\
NSE 1976 vol 27, 607 - 608 (two more approximations are given)\
\>", "Subsection",
 CellChangeTimes->{{3.5830385023570423`*^9, 3.583038567537313*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"H32", "[", 
   RowBox[{"c_", ",", "u_"}], "]"}], ":=", 
  RowBox[{
   SuperscriptBox[
    RowBox[{"(", 
     RowBox[{"1", "-", 
      RowBox[{
       FractionBox["c", "2"], 
       RowBox[{"(", 
        RowBox[{
         RowBox[{
          RowBox[{"(", 
           RowBox[{"1", "-", 
            RowBox[{"A", " ", "u"}], "+", 
            RowBox[{"B", " ", 
             SuperscriptBox["u", "2"]}]}], ")"}], "u", " ", 
          RowBox[{"Log", "[", 
           FractionBox[
            RowBox[{"1", "+", "u"}], "u"], "]"}]}], "+", 
         RowBox[{"A", " ", "u"}], " ", "+", " ", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{
            FractionBox["u", "2"], "-", 
            SuperscriptBox["u", "2"]}], ")"}], "B"}]}], ")"}]}]}], ")"}], 
    RowBox[{"-", "1"}]], "//.", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"A", "\[Rule]", 
      RowBox[{"\[Alpha]", "-", 
       RowBox[{
        FractionBox["2", "3"], "B"}]}]}], ",", 
     RowBox[{"B", "\[Rule]", 
      FractionBox[
       RowBox[{
        RowBox[{"K", " ", 
         RowBox[{"Log", "[", 
          RowBox[{"1", "+", "K"}], "]"}]}], "+", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"K", "+", 
           RowBox[{"Log", "[", 
            RowBox[{"1", "-", "K"}], "]"}]}], ")"}], "\[Alpha]"}]}], 
       RowBox[{
        FractionBox["K", "b"], "+", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{
           FractionBox["2", "3"], "-", 
           FractionBox["1", "K"]}], ")"}], 
         RowBox[{"Log", "[", 
          RowBox[{"1", "-", "K"}], "]"}]}], "-", "1"}]]}], ",", 
     RowBox[{"\[Alpha]", "\[Rule]", 
      RowBox[{
       RowBox[{
        FractionBox["4", "c"], 
        RowBox[{"(", 
         RowBox[{"1", "-", 
          SqrtBox[
           RowBox[{"1", "-", "c"}]]}], ")"}]}], "-", "2"}]}], ",", 
     RowBox[{"b", "\[Rule]", 
      RowBox[{"1", "/", 
       RowBox[{"(", 
        RowBox[{
         FractionBox[
          SqrtBox[
           RowBox[{"1", "-", "c"}]], 
          RowBox[{"2", "K"}]], "-", 
         RowBox[{
          FractionBox["c", 
           RowBox[{"4", " ", 
            SuperscriptBox["K", "2"]}]], 
          RowBox[{"Log", "[", 
           RowBox[{"1", "-", 
            SuperscriptBox["K", "2"]}], "]"}]}]}], ")"}]}]}], ",", 
     RowBox[{"K", "\[Rule]", 
      RowBox[{"vexact", "[", "c", "]"}]}]}], "}"}]}]}]], "Input",
 CellChangeTimes->{{3.583037475943775*^9, 3.583037479110606*^9}, {
   3.5830376141410723`*^9, 3.583037902113928*^9}, 3.583037955799624*^9, {
