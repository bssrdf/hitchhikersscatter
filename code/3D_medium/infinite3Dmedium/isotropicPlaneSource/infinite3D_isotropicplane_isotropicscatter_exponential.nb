Cell[BoxData[
 RowBox[{
  RowBox[{"inf3Disoplaneisoscatter`\[Phi]exact1", "[", 
   RowBox[{"x_", ",", "\[CapitalSigma]t_", ",", "\[Alpha]_", ",", "n_"}], 
